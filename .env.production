VITE_STRIPE_PRICE_TEAM_PRO_SEAT=

# Application Configuration (Required)
VITE_APP_NAME=Setique Founder Dashboard
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production
VITE_APP_URL=https://founderhq.setique.com

# Error Tracking & Monitoring (Highly Recommended)
VITE_SENTRY_DSN=https://1da55fe762067514256c2907c2f62de3@o4510327406526464.ingest.us.sentry.io/4510327478943744

# Sentry Source Map Upload (Set in Netlify only - DO NOT commit to git)
